  "description": "Candy Coin Backend",
  "main": "app.js",
  "scripts": {
    "start": "nodemon",
    "start:prod": "NODE_PATH=./dist node dist/app.js",
    "lint": "eslint '*/**/*.ts'",
    "prettier:write": "prettier --write src/**/*.*",
    "clean": "rimraf ./dist",
    "copy:assets": "cp -rv  src/views dist/views",
    "build": "npm run clean && tsc --project tsconfig.build.json && npm run copy:assets",
    "docker:build": "docker build -t candy-coin-api -f Dockerfile .",
    "docker:run": "docker run -d --name candy-coin-api -p 3001:3000 --env-file ./.env.dist candy-coin-api",
    "docker:stop": "docker stop candy-coin-api && docker rm candy-coin-api",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
